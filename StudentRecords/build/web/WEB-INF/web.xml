<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>com.servlets.Index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cadastro</servlet-name>
        <servlet-class>com.servlets.Cadastro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sobre</servlet-name>
        <servlet-class>com.servlets.Sobre</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contato</servlet-name>
        <servlet-class>com.servlets.Contato</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Entrar</servlet-name>
        <servlet-class>com.servlets.Entrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Remover</servlet-name>
        <servlet-class>com.servlets.Remover</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cadastro</servlet-name>
        <url-pattern>/Cadastro</url-pattern>
        <url-pattern>/cadastro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sobre</servlet-name>
        <url-pattern>/Sobre</url-pattern>
        <url-pattern>/sobre</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contato</servlet-name>
        <url-pattern>/Contato</url-pattern>
        <url-pattern>/contato</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Entrar</servlet-name>
        <url-pattern>/Entrar</url-pattern>
        <url-pattern>/entrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Remover</servlet-name>
        <url-pattern>/Remover</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Index</welcome-file>
    </welcome-file-list>
</web-app>
